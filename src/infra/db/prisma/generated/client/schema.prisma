generator client {
  provider = "prisma-client-js"
  output   = "../src/infra/db/prisma/generated/client"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Ticket {
  id         Int      @id @default(autoincrement())
  setor      String
  nucleo     String
  problema   String
  solucao    String?
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
